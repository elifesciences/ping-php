{
    "name": "elife/ping",
    "description": "eLife Sciences ping controller",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "eLife\\Ping\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\eLife\\Ping\\": "./test"
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/http-foundation": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "pimple/pimple": "^3.0",
        "silex/silex": "^2.2",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "^3.0",
        "symfony/config": "^3.0",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-kernel": "^3.0"
    },
    "suggest": {
        "silex/silex": "^2.2, to use PingControllerProvider",
        "symfony/http-kernel": "^3.0, to use PingBundle"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
